{
  "version": 2,
  "builds": [
    {
      "src": "main.py",        
      "use": "@vercel/python"  
    }
  ],
  "functions": {
    "api": {
      "handler": "api/main.py",
      "runtime": "python3.8"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    }
  ]
}
